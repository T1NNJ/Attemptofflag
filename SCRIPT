local function pressKey()
wait(.5)  
game:GetService("VirtualInputManager"):SendKeyEvent(true, Enum.KeyCode.KeypadSeven, false, game)
end

if Pressing then
    Pressing = false
    stopFlag = true
else
    Pressing = true
    stopFlag = false
    spawn(function()
        repeat
            pressKey()
            wait(1)
            game:GetService("RunService").Heartbeat:Wait()
        until stopFlag or not Pressing
    end)
end
end)

