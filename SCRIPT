local speed = 10
while true do
	for i = 0,1,0.001*speed do
		game:GetService("Players").LocalPlayer.PlayerGui.Sprinting.Frame.Frame.BackgroundColor3 = Color3.fromHSV(i,1,1)
		wait()
	end
end
end)

